<%=
  form_for :feeds, routes.feed_delete_path(parsed_feed[:id]), method: :delete, 'data-scrollfreeze': true do
    div class: 'delete-feed-form' do
      div class: 'gra-form-actions' do
        submit 'Delete', class: 'gra-btn'
        a 'Back', class: 'gra-btn gra-btn-outline', 'data-controller': 'delete',
          'data-action': 'click->delete#hide', 'data-id': parsed_feed[:id]
      end
    end
  end
%>
